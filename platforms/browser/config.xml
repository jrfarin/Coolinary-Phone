<?xml version='1.0' encoding='utf-8'?>
<widget id="es.coolinary.webflix" version="1.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Coolinary MC's</name>
    <description>
        Coolinary Market Master Classes &amp; 2017 Recipes App.
    </description>
    <author email="coolinary@webflix.es" href="http://webflix.coolinary.com">
        JRFarin
    </author>
    <content src="index.html" />
    <access origin="http://maps.google.com" />
    <access origin="https://maps.google.com" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.3.0" />
</widget>
